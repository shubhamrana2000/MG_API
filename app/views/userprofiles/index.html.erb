
<div class="d-flex justify-content-between align-items-center">
  <h1>Userprofiles</h1>
  <%= link_to "New userprofile", new_userprofile_path,class:"btn btn-outline-primary" %>
</div>

<div id="userprofiles">
  <table class="table table-hover">
    <thead style="background: #7952b3;color:white">
      <tr>
        <th>S.No</th>
        <th>Email </th>
        <th>Name </th>
        <th>Phone Number </th>
        <th>Address</th>
        <th>DOB</th>
        <th>Occupation</th>
        <th>Gender</th>
        <th>ProfilePic</th>
        <th>Show</th>
      </tr>
    </thead>
    <tbody>
      <% @userprofiles.each_with_index do |userprofile,index| %>
        <tr>
          <td><%= index+1 %></td>
          <%= render userprofile %>
          <td><%= link_to "Show profile", userprofile,class:"btn btn-outline-warning" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

